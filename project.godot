; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Admob",
"language": "GDScript",
"path": "res://admob-lib/admob.gd"
} ]
_global_script_class_icons={
"Admob": "res://admob-lib/icon.png"
}

[android]

modules="org/godotengine/godot/GodotAdMob"

[application]

config/name="Number Gussing"
run/main_scene="res://Main.tscn"
boot_splash/image="res://ADVENT_LOGO_FINAL-removebg-preview.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://Icons/Android/g5037.png"

[display]

window/size/width=144
window/size/height=256
window/size/test_width=288
window/size/test_height=512
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[rendering]

environment/default_clear_color=Color( 0.188235, 0.188235, 0.188235, 1 )
environment/default_environment="res://default_env.tres"
